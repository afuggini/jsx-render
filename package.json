{
  "name": "jsx-render",
  "version": "1.1.2",
  "description": "",
  "browser": "build/dom.js",
  "main": "lib/dom.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alecsgone/jsx-render.git"
  },
  "keywords": [
    "JSX",
    "jsx-render",
    "SVG-JSX"
  ],
  "author": "Aleks",
  "bugs": {
    "url": "https://github.com/alecsgone/jsx-render/issues"
  },
  "homepage": "https://github.com/alecsgone/jsx-render#readme",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config examples/webpack.config.js --hot",
    "dev": "babel src --out-dir lib --watch",
    "build": "babel src --out-dir lib & webpack",
    "eslint": "eslint src/.",
    "prepare": "npm run test && npm run build",
    "prepublish": "npm run test && npm run build",
    "posttest": "npm run eslint",
    "test": "ava test/*.* --verbose"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "ava": {
    "require": [
      "@babel/register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.2.1",
    "babel-loader": "^8.0.5",
    "babel-preset-primavera": "^1.3.0",
    "browser-env": "^3.2.6",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "redux": "^4.0.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.1.14"
  },
  "optionalDependencies": {
    "cheerio": "^1.0.0-rc.3"
  }
}
